{
  "$schema": "res/schema.json",
  "alias": "interceptor.phar",
  "chmod": "0755",
  "directories": [
    "src"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "tests",
        "Tests"
      ],
      "in": "vendor"
    }
  ],
  "git-version": "package_version",
  "main": "interceptor.php",
  "output": "interceptor.phar",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Php",
    "KevinGH\\Box\\Compactor\\PhpScoper",
    "KevinGH\\Box\\Compactor\\Json"
  ],
  "compression": "GZ",
  "stub": true
}